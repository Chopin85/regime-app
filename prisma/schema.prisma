generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Food {
  id           String     @id @default(cuid())
  name         String
  kcal         Int
  image        String?
  link         String
  Constumers   Constumers @relation(fields: [constumersId], references: [id])
  constumersId String
}

model Constumers {
  id   String @id @default(cuid())
  name String
  Food Food[]
}
