// schema.prisma

generator client {
  provider = "prisma-client-js"
  binaryTargets   = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
}

model Food {
  id        String     @default(cuid()) @id
  name      String
  kcal      Int
  image     String?
  link      String
}

